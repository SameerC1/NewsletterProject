#imports needed
import smtplib
import imaplib
from email.message import EmailMessage
from methods import makemail,sendmail



#creates the email for the newsletter
msg=makemail()
#Connects to SMTP
connection=smtplib.SMTP_SSL('smtp.gmail.com',465)
email=input("Enter email here")
password=input("enter password here")
connection.login(email,password)
#Sends the email
sendmail(msg,connection)
